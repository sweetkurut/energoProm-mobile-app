{
    "expo": {
        "name": "Energo",
        "slug": "Energo",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon1024.png",
        "scheme": "energoprommobileapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/512.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true,
            "icon": "./assets/images/icon1024.png",
            "bundleIdentifier": "com.devxwc.energo",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/iconpng1024.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.devxwc.energo"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/iconpng1024.png"
        },
        "plugins": ["expo-router", "expo-font", "expo-secure-store"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "728b2476-bfe0-4e21-8239-2a234772cab2"
            }
        }
    }
}
